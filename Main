import java.util.Random;

Public class Main {
    public static void main(String[] args) {
    Lista lista = new Lista();
    
    
    Lista.insertar(12);
    Lista.insertar(4);
    Lista.insertar(111);
    Lista.quickSort();
    Lista.recorrer();
    
    
    for(int i = 0; i < 1000001; ++i){
        Random R = new Random();
        int x = R.nextInt(100001);


        Lista.insertar(x);
    }


    Lista.quickSort();
    Lista.recorrer();


    }
}
